<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.miwok">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        tools:replace="android:label"
        android:label="Beginner English Tutorial"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Numbers"
            android:label="Numbers" />
        <activity
            android:name=".Family_members"
            android:label="Family Members" />
        <activity
            android:name=".Colors"
            android:label="Colors" />
        <activity
            android:name=".Phrases"
            android:label="Phrases" />
        <activity android:name=".Questioning" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".Lesson_content"
            android:label="@string/title_activity_lesson_content"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>